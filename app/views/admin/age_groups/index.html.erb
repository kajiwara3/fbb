<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th><%= AgeGroup.human_attribute_name 'age_group_code' %></th>
      <th><%= AgeGroup.human_attribute_name 'caption' %></th>
    </tr>
  </thead>
  <tbody>
    <% @age_groups.each do |age| %>
      <tr>
        <td><%= age.age_group_code %></td>
        <td><%= age.caption %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= paginate @age_groups %>
